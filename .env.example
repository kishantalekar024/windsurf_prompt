# Example environment configuration
# Copy this to .env and customize

# Database Configuration
DATABASE_TYPE=postgresql  # postgresql or mongodb
DATABASE_URL=postgresql://username:password@localhost:5432/windsurf_prompts
MONGO_URL=mongodb://localhost:27017/windsurf_prompts

# Proxy Configuration
PROXY_PORT=8080
ENABLE_WEB_INTERFACE=true
WEB_PORT=5000

# AI API Endpoints to Monitor
MONITOR_OPENAI=true
MONITOR_ANTHROPIC=true
MONITOR_CODEIUM=true
MONITOR_ALL_AI_APIS=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/windsurf_interceptor.log

# SSL Certificate
AUTO_INSTALL_CERT=true
CERT_PATH=~/.mitmproxy/mitmproxy-ca-cert.pem

# Dashboard Authentication
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=changeme123